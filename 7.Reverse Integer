/**
 * @param {number} x
 * @return {number}
 */
var reverse = function(x) {
    var nums = [];
    var num;
    var sum = 0;
    num = x;
    for(var i=0;;i++){
        nums[i] = num%10;
        num = Math.floor(num/10);
        if(num < 1){
            break;
        }
    }
    for(var j=0;j<nums.length;j++){
        sum = sum + nums[j]*Math.pow(10,(nums.length-j-1));   
    }
    return sum;
};
